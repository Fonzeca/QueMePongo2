<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="NVARCHAR"
			hibernate-type='java.lang.String'>
		</sql-type>
	</type-mapping>
	<table-filter match-catalog="QueMePongo2" match-schema="dbo"
		match-name=".*">
	</table-filter>
	<table-filter match-name="sysdiagrams" match-catalog="QueMePongo2"
		match-schema="dbo" exclude="true">
	</table-filter>
</hibernate-reverse-engineering>